{
	"name": "oauth-flow-sveltekit",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm run dev:css\" \"vite dev\"",
		"dev:css": "tailwindcss -w -i ./src/tailwind.css -o static/app.css",
		"build": "npm run build:css && vite build",
		"build:css": "tailwindcss -m -i ./src/tailwind.css -o static/app.css",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.2",
		"@types/tcp-port-used": "^1.0.1",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.24",
		"postgres": "^3.3.5",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"vite": "^4.3.9"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.10.1",
		"@supabase/supabase-js": "^2.25.0",
		"daisyui": "^2.51.5",
		"zod": "^3.21.4"
	}
}
