{
	"name": "reset-flow",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm run dev:css\" \"vite dev\"",
		"dev:css": "tailwindcss -w -i ./src/tailwind.css -o static/app.css",
		"build": "npm run build:css && vite build",
		"build:css": "tailwindcss -m -i ./src/tailwind.css -o static/app.css",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@playwright/test": "^1.32.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.0",
		"@types/tcp-port-used": "^1.0.1",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.0.1",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.19",
		"postgres": "^3.3.4",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.1.4",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.3.1",
		"tcp-port-used": "^1.0.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.3",
		"vite": "^4.2.1"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.9.0",
		"@supabase/supabase-js": "^2.14.0",
		"daisyui": "^2.51.5",
		"zod": "^3.21.4"
	}
}
