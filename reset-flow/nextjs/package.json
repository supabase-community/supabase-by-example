{
  "name": "reset-flow-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"next dev\"",
    "dev:css": "tailwindcss -w -i ./styles/tailwind.css -o styles/app.css",
    "build": "npm run build:css && next build",
    "build:css": "tailwindcss -m -i ./styles/tailwind.css -o styles/app.css",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test"
  },
  "dependencies": {
    "@next/font": "13.2.4",
    "@supabase/auth-helpers-nextjs": "0.7.0-next.3",
    "@supabase/auth-helpers-react": "0.4.0-next.0",
    "@supabase/supabase-js": "^2.21.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "daisyui": "^2.51.5",
    "next": "13.2.4",
    "postcss": "^8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.1",
    "typescript": "5.0.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.32.1",
    "@types/tcp-port-used": "^1.0.1",
    "tcp-port-used": "^1.0.2"
  }
}
