{
	"name": "user-profile",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm run dev:css\" \"vite dev\"",
		"dev:css": "tailwindcss -w -i ./src/tailwind.css -o static/app.css",
		"build": "npm run build:css && vite build",
		"build:css": "tailwindcss -m -i ./src/tailwind.css -o static/app.css",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.10.0",
		"@types/tcp-port-used": "^1.0.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.13",
		"child-command": "^1.0.3",
		"concurrently": "^7.6.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"node-async-exec": "^1.2.0",
		"postcss": "^8.4.19",
		"postgres": "^3.3.3",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.4",
		"svelte-preprocess": "^5.0.1",
		"tailwindcss": "^3.2.4",
		"tcp-port-used": "^1.0.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.5",
		"vite": "^4.0.1"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.9.0",
		"@supabase/supabase-js": "^2.12.1",
		"daisyui": "^2.41.0",
		"zod": "^3.19.1"
	}
}
