{
  "name": "express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"nodemon ./bin/www\"",
    "start": "node ./bin/www",
    "dev:css": "tailwindcss -w -i ./public/stylesheets/tailwind.css -o public/stylesheets/style.css",
    "build": "npm run build:css",
    "build:css": "tailwindcss -m -i ./public/stylesheets/tailwind.css -o public/stylesheets/app.css",
    "test": "playwright test"
  },
  "dependencies": {
    "@peculiar/webcrypto": "^1.4.3",
    "@supabase/auth-helpers-shared": "^0.4.1",
    "cookie-parser": "~1.4.4",
    "daisyui": "^3.1.0",
    "debug": "~2.6.9",
    "dotenv": "^16.1.4",
    "express": "~4.16.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "3.0.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.35.0",
    "@types/tcp-port-used": "^1.0.1",
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "tailwindcss": "^3.3.2",
    "tcp-port-used": "^1.0.2"
  }
}
